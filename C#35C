namespace arrar
{
	class progarray
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
			char [] x=new char[5];
			string[] t=new string[4];
			t[0]="ali";
			t[1]="alexander";
			t[2]="tomy";
			t[3]="jhon";
			x[0]='A';
			x[1]='B';
			x[2]='C';
			x[3]='D';
			x[4]='E';
			for(int i=0;i<4;i++)
			{
				Console.WriteLine("The Name is: " + t[i] + "\nThe Mark is= " + x[i] );
			}
			Console.ReadKey();
		}
	}
}




