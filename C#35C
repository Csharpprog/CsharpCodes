using System;
namespace chsarp
{
class sharp
{
static void Main(string[] args)
{
Console.Title="C# ";
Console.BackgroundColor=ConsoleColor.Black;
Console.ForegroundColor=ConsoleColor.Yellow;
int x,Res;
Console.Write("Enter x= ");
x=int.Parse(Console.ReadLine());
Res=Math.Abs(x);
Console.Write("The Absolution of x = " + Res);
Console.ReadKey();
}
}
}
___________________________________________________________________________

using System;
namespace chsarp
{
    class sgatore
    {
        static void Main(string[] args)
        {
            Console.Title="C# ";
            Console.BackgroundColor=ConsoleColor.Black;
            Console.ForegroundColor=ConsoleColor.Yellow;
            double x,Res,Ras,y;
            Console.Write("Enter x= ");
            x=double.Parse(Console.ReadLine());
            Res=Math.Ceiling(x);
            Console.Write("The Ceiling for x= " + Res);
            Console.Write("\nEnter y= ");
            y=double.Parse(Console.ReadLine());
            Ras=Math.Ceiling(y);
            Console.Write("The Ceiling for y= " + Ras);
            Console.ReadKey();
        }
    }
}
