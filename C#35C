namespace sharp
{
	class cshrp
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
			int i,sum=0,n;
			Console.WriteLine("Enter students number= ");
			n=int.Parse(Console.ReadLine());
			Console.Write("numbers are=");
			for(i=1;i<=n;i++)
			{
				Console.Write(" " +i);
			}
			for(i=1;i<=n;i++)
			{
				if(i==5)
					continue;
				sum+=i;
			}
			Console.Write("\nThe sumation is= " + sum);
			Console.ReadKey();
		}
	}
}



