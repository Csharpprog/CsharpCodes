using System;
namespace array
{
	class csharry
	{
		static void Main(string[] args)
		{
			Console.Title="C# Array";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
			int i,j,t;
			int [] x=new int[5];
			Console.WriteLine("Enter 5 values for array= ");
			for(i=0;i<x.Length;i++)
			{
				x[i]=int.Parse(Console.ReadLine());
			}
			for(i=0;i<x.Length;i++)
			for(j=i+1;j<x.Length;j++)
			{
				if(x[i]>x[j])
				{
					t=x[i];
					x[i]=x[j];
					x[j]=t;
				}
			}
			Console.Write("The array is= ");
			for(i=0;i<x.Length;i++)
			{
				Console.Write(x[i] + " , ");
			}
			Console.ReadKey();
		}
	}
}




using System;
namespace csharp
{
    class sharp
    {
        static void Main(string[] args)
        {
            Console.Title="C# ";
            Console.BackgroundColor=ConsoleColor.Black;
            Console.ForegroundColor=ConsoleColor.Yellow;
            int i, j, n,l=1,c=0;
            int[] x = new int[100];
            int[] y = new int[100];
            int[] w = new int[100];
            Console.Write("Input the number of elements to be stored in the array :");
            n = int.Parse(Console.ReadLine());
            Console.WriteLine("Input {0} elements in the array :\n" ,n);
            for (i = 0; i < n; i++)
            {
                Console.Write("element - {0} : " , i);
                x[i] = int.Parse(Console.ReadLine());
            }
            for (i = 0; i < n; i++)
            {
                y[i] = x[i];
                w[i] = 0;
            }
            for (i = 0; i < n; i++)
            {
                for (j = 0; j < n; j++)
                {
                    if (x[i] == x[j])
                    {
                        w[j] = l;
                        l++;
                    }
                }
                l = 1;
            }
            for (i = 0; i < n; i++)
            {
                if (w[i] == 1)
                {
                    c++;
                }   
            }
            Console.Write("Total number of duplicate elements found in the array is : " + c);
            Console.Write("\nIndex of array elements are:");
            for (i = 0; i < n; i++)
            {
            	if (w[i] == 2)
                {
            	Console.Write(x[i] + " ");
            	}
            }
            Console.ReadKey();
        }
    } 	
}




using System;
namespace csharp
{
    class sharp
    {
        static void Main(string[] args)
        {
            Console.Title="C# ";
            Console.BackgroundColor=ConsoleColor.Black;
            Console.ForegroundColor=ConsoleColor.Yellow;
            int i, j,n;
            int[] x = new int[100];
            int[] y = new int[100];
            int[] w = new int[100];
            Console.Write("Input the number of elements to be stored in the array :");
            n = int.Parse(Console.ReadLine());
            Console.WriteLine("Input {0} elements in the array :\n" ,n);
            for (i = 0; i < n; i++)
            {
                Console.Write("element - {0} : " , i);
                x[i] = int.Parse(Console.ReadLine());
            }
            Console.Write("The Even elements are : \n");
            for (i = 0; i < n; i++)
            {
                if (x[i] % 2 == 0)
                {
                    y[i] = x[i];
                    Console.Write(y[i] + " ");
                }
            }
            Console.Write("\nThe Odd elements are : \n");
            for (i = 0; i < n; i++)
            {
                if (x[i] % 2 != 0)
                {
                    w[i] = x[i];
                    Console.Write(w[i]+ " ");
                }
            }
            Console.ReadKey();
        }
    } 	
}




using System;
namespace ssharp
{
class csharp
{
static void Main(string[] args)
{
Console.Title="C# ";
Console.BackgroundColor=ConsoleColor.Black;
Console.ForegroundColor=ConsoleColor.Yellow;
int i;
char [] t=new char[4];
Console.Write("Enter any name= \n");
for(i=0;i<t.Length;i++)
{
	t[i]=char.Parse(Console.ReadLine());
}
Console.Write("\nThe name in reverse order is= \n");
for(i=t.Length-1;i>=0;i--)
{
Console.Write(t[i]);
}
Console.ReadKey();
}
}
}



