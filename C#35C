namespace starthome
{
	class progbegin
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
			int i,x;
			Console.Write("Enter the number you want to print numbers less than or equal to it in reverse order : ");
			x=int.Parse(Console.ReadLine());
			Console.Write("Input a number: ");
			for(i=1;i<=x;i++)
			{
				Console.Write(i);
			}
			Console.Write("\nThe number in reverse order is : ");
			for(i=x;i>=1;i--)
			{
				Console.Write(i);
			}
			Console.ReadKey();
		}
	}
}




using System;
namespace sharp
{
	class cshrp
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
			int i,j,s,r;
			Console.Write("Enter number of rows= ");
			r=int.Parse(Console.ReadLine());
			s=r+4-1;
			for(i=1;i<=r;i++)
			{
				for(j=s;j>=1;j--)
				Console.Write(" ");
				for(j=1;j<=i;j++)
				Console.Write("*");
				Console.Write("\n");
				s--;
			}
			Console.ReadKey();
		}
	}
}




using System;
namespace sharp
{
	class cshrp
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
			int i,n,ctr=0;
			Console.WriteLine("Enter any number= ");
			n=int.Parse(Console.ReadLine());
			for(i=2;i<=n/2;i++)
			{
				if(n%i==0)
				{
					ctr++;
					break;
				}
			}
			if(ctr==0 && ctr!=1)
			Console.WriteLine(n + " is a Prime number ");
			else
			Console.WriteLine(n + " is not a Prime number ");
			Console.ReadKey();
		}
	}
}




using System;
namespace sharp
{
	class cshrp
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
			int i,j,bin=0,dc,n;
			Console.WriteLine("Enter number= ");
			n=int.Parse(Console.ReadLine());
			dc=n;
			i=1;
			for(j=n;j>0;j=j/2)
			{
				bin=bin+(n%2)*i;
				i=i*10;
				n=n/2;
			}
			Console.Write("The decimal number is= " + dc + "\nThe binary number is= " + bin);
			Console.ReadKey();
		}
	}
}




