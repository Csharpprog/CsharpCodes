        {
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int i=0,j=0;
            do
            {
            	++i;
            	for(;j<=10;j++)
            	{
            		Console.WriteLine(i + "*" + j + "=" + (i*j));
            	}
            }
            while (i>=0 && i<=10);
            Console.ReadKey();
        }
    }
