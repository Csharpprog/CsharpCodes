using System;

namespace sharp
{
    class csharp
    {
        static void Main(string[] args)
        {
            Console.Title="C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int i, j;
            for (i=10;i>=0;i--)
            {
                for(j=0;j<=10-i;j++)
                    Console.Write(" ");
                for(j=0;j<=i;j++)
                    Console.Write("*");
                for(j=i-0;j>=1;j--)
                    Console.Write("*");
                Console.Write("\n");
            }
            Console.ReadKey();
        }
    }
}



using System;
namespace sharp
{
    class cshrp
    {
        static void Main(string[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int i,j,r,s,count=1,l;
            Console.Write("Enter number of rows= ");
            r=int.Parse(Console.ReadLine());
            s=r+4-1;
            for(i=0;i<=r;i++)
            {
            for(l=s;l>=1;l--)
            {
            Console.Write(" ");
            }
            	for(j=1;j<=i;j++)
            		Console.Write(count++ + " ");
            	    Console.Write("\n");
            	    s--;
            }
            Console.ReadKey();
        }
      }
    }




