using System;
using System.Runtime.CompilerServices;

namespace Csharp
{
    class Csharp
    {
        static void Main(string[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor=ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int i, j;
            for (i = 10; i > 0; i--)
            {
                for (j = 0; j < 10-i; j++)
                    Console.Write(" ");
                for(j=0;j<i;j++)
                    Console.Write("*");
                    Console.Write("\n");
            }
            Console.ReadKey();
        }
    }
}
