using System;
namespace schsarp
{
    class csharp
    {
        struct pop
        {
            public string n;
            public int x;
        }
        static void Main(string[] args)
        {
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            pop p=new pop();
            Console.Write("Enter your Name: ");
            p.n = Console.ReadLine();
            Console.Write("Enter your age: ");
            p.x = int.Parse(Console.ReadLine());
            Console.Write("\nYour Name is : " + p.n + "\nYour age is : " + p.x );
            Console.ReadKey();
        }
    }
}


_____________________________________________________________________________________

using System;
namespace schsarp
{
    class csharp
    {
        enum pop
        {
            jhone,tomy
        }
        static void Main(string[] args)
        {
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            string t;
            Console.Write("Enter your choice : ");
            t = Console.ReadLine();
            switch (t)
            {
                    case "jhone" : Console.Write("You are a British Man: " + pop.jhone);
                        break;
                    case"tomy":Console.Write("You are an American Man: " + pop.tomy);
                            break;
                    default : Console.Write("You are enter a wrong choice");
                        break;
            }
            Console.ReadKey();
        }
    }
}


