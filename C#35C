using System;
namespace Csharp
{
    class Csharp
    {
        static void Main(string[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor=ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int i, n, sum = 0;
            Console.Write("Enter n= ");
            n = int.Parse(Console.ReadLine());
            for (i = 1; i < n; i++)
            {
                if (i == 5)
                    continue;
                sum += i;
            }
            Console.Write(sum);
            Console.ReadKey();
        }
    }
}
________________________________________________________________________________________________________________________________________

using System;
namespace Csharp
{
    class Csharp
    {
        static void Main(string[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor=ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int i, n;
            Console.Write("Enter n= ");
            n = int.Parse(Console.ReadLine());
            int[] x = new int[n];
            for (i = 0; i < x.Length; i++)
            {
                x[i] = int.Parse(Console.ReadLine());
            }
            Console.Write("\nThe array in reverse order is= ");
            for (i = x.Length - 1; i >= 0; i--)
            {
                Console.Write(x[i] + " ");
            }
            Console.ReadKey();
        }
    }
}


_________________________________________________________________________________________


using System;
namespace Csharp
{
    class Csharp
    {
        static void Main(string[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor=ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            Console.Write("Enter the array\n");
            int[,] x = new int[3,3];
            int[,] y = new int[3,3];
            int i, j;
            for (i = 0; i < x.GetLength(0); i++)
            {
                for (j = 0; j < x.GetLength(1); j++)
                {
                    Console.Write("Element_[{0},{1}] ",i,j);
                    x[i, j] = int.Parse(Console.ReadLine());
                }
            }
            Console.Write("The even elements= ");
            for (i = 0; i < x.GetLength(0); i++)
            {
                for (j = 0; j < x.GetLength(1); j++)
                {
                    if(x[i,j]%2==0)
                        Console.Write(x[i,j] + " ");
                }
            }
            Console.Write("\nThe odd elements= ");
            for (i = 0; i < x.GetLength(0); i++)
            {
                for (j = 0; j < x.GetLength(1); j++)
                {
                    if(x[i,j]%2!=0)
                        Console.Write(x[i,j] + " ");
                }
            }
            Console.ReadKey();
        }
    }
}
___________________________________________________________________________________________________



using System;
class Mul
{
    static void Main()
    {
        Console.Title="C# ";
        Console.BackgroundColor=ConsoleColor.Black;
        Console.ForegroundColor=ConsoleColor.Yellow;
        int i,j;
        Console.WriteLine("Multiplication Table :");
        for (i = 1; i <= 6; i++)
         {
            Console.WriteLine("\n");
            for (j = 1; j <= 6; j++)

            {
                Console.Write("{0,6}", i * j);
            } 
        }
        Console.ReadKey();
    }
}

________________________________________________________________________________________________


using System;
class Mul
{
    static void Main()
    {
        Console.Title="C# ";
        Console.BackgroundColor=ConsoleColor.Black;
        Console.ForegroundColor=ConsoleColor.Yellow;
        int i,j;
        Console.WriteLine("Multiplication Table :");
        for (i = 0; i <= 6; i++)
         {
            Console.WriteLine("\n");
            for (j = 0; j <= 6; j++)
            {
                Console.Write("{0,6}", i + j);
            } 
        }
        Console.ReadKey();
    }
}

