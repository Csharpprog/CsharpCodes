using System;
namespace oklk
{
	class dolk
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
			int i,x,sum=0;
			Console.WriteLine("Enter the lowest value of x= ");
			x=int.Parse(Console.ReadLine());
			for(i=0;i<=x;i+=2)
			{
				sum+=i;
			}
			Console.Write("The summation values= ");
			for(i=x;i>=0;i--)
			{
				Console.Write("\n" + (i--) );
			}
			Console.WriteLine("\nThe summation values under= "+ x +" is= "+sum);
			Console.ReadKey();
		}
	}
}




using System;
namespace PrimeNumber
{
    class ProgrSharp
    {
        static void Main(string[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
        	int n, i, res;
            bool flag = true;

            Console.WriteLine("Please Enter any Number= ");
            n = int.Parse(Console.ReadLine());
            for (i = 2; i <= n / 2; i++)
            {
                res = n % i;
                if (res == 0)
                {
                    flag = false;
                    break;
                }
            }
            if (flag)
                Console.WriteLine(n + " is Prime Number");
		    else
                Console.WriteLine(n + " is not Prime Number");

            Console.ReadLine();
        }
    }
}




using System;
namespace csharp
{
	class sharp
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
			int i,j;
			for(i=6;i>=0;i--)
			{
				for(j=6;j>=i;j--)
					Console.Write(" ");
				    Console.Write("*");
				    Console.Write("\n");
			}
			for(i=0;i<=6;i++)
			{
			    for(j=6;j>=i;j--)
					Console.Write(" ");
				    Console.Write("*");
				    Console.Write("\n");	
			}
			Console.ReadKey();
		}
	}
}




