using System;
namespace beging
{
    class program
    {
        static void Main(string[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Cyan;
            Console.ForegroundColor = ConsoleColor.Black;
            Console.WriteLine(" Welcome to C# This is the first project ");
            int x, y, z;
            float Res,avg;
            Console.WriteLine("Enter the first value = ");
            x = int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the second value = ");
            y = int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the third value = ");
            z = int.Parse(Console.ReadLine());
            Res = x + y + z;
            Console.WriteLine("The result of Summation is = ");
            Console.WriteLine(Res);
            avg = Res / 3;
            Console.WriteLine("The Average is = ");
            Console.WriteLine(avg);
            Console.ReadKey();
        }
    }
}




using System;
namespace beging
{
    class program
    {
        static void Main(string[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Cyan;
            Console.ForegroundColor = ConsoleColor.Black;
            Console.WriteLine(" Welcome to C# This is the first project ");
            string w;
            int x, y, z;
            float Res,avg;
            Console.WriteLine("Enter the first value = ");
            x = int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the second value = ");
            y = int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the third value = ");
            z = int.Parse(Console.ReadLine());
            Res = x + y + z;
            Console.WriteLine("The result of Summation is = ");
            Console.WriteLine(Res);
            avg = Res / 3;
            Console.WriteLine("Enter the Student Name: ");
            w = Console.ReadLine();
            Console.Write("The Average of "+ w +"\n");
            Console.Write(avg);
            Console.ReadKey();
        }
    }
}




using System;
namespace beging
{
    class program
    {
        static void Main(string[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Cyan;
            Console.ForegroundColor = ConsoleColor.Black;
            Console.WriteLine(" Welcome to C# This is the first project ");
            int x, y, z;
            float Res,avg;
            Console.WriteLine("Enter the first value = ");
            x = int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the second value = ");
            y = int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the third value = ");
            z = int.Parse(Console.ReadLine());
            Res = x + y + z;
            Console.WriteLine("The result of Summation is = ");
            Console.WriteLine(Res);
            avg = Res / 3;
            Console.Write("The Average is = \n");
            Console.Write(avg);
            
            Console.ReadKey();
        }
    }
}




using System;
namespace firstb
{
	class startfir
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Cyan;
			Console.ForegroundColor=ConsoleColor.Black;
			int x,y,w,z,Res,avg;
			Console.WriteLine("Enter the first value= ");
			x=int.Parse(Console.ReadLine());
			Console.WriteLine("Enter the second value= ");
			y=int.Parse(Console.ReadLine());
			Console.WriteLine("Enter the third value= ");
			w=int.Parse(Console.ReadLine());
			Console.WriteLine("Enter the forth value= ");
			z=int.Parse(Console.ReadLine());
			Console.Write("\n");
			Console.WriteLine("the first v={0} \nthe second v={1} \nthe third v={2} \nthe forth v={3} \n",x,y,w,z);
			Res=x+y+w+z;
			Console.WriteLine("The result is= \n"+ Res);
			avg=Res/4;
			Console.WriteLine("The Average is= \n"+ avg);
			Console.ReadKey();
		}
	}
}




using System;
namespace Cgoole
{
	class progrm
	{
		static void Main(string[] args)
		{
		Console.Title="C# ";
		Console.BackgroundColor= ConsoleColor.Cyan;
		Console.ForegroundColor=ConsoleColor.Black;
		int x,y,z,w;
		Console.Write("Enter the first value= ");
		x=int.Parse(Console.ReadLine());
		Console.Write("Enter the second value= ");
		y=int.Parse(Console.ReadLine());
		Console.Write("Enter the third value= ");
		z=int.Parse(Console.ReadLine());
		w=x*y*z;
		Console.WriteLine("\n");
		Console.Write("The result of multiplicationis="+ " " + w);
		Console.ReadKey();
		
		}
	}
}



using System;
namespace Cgoole
{
	class progrm
	{
		static void Main(string[] args)
		{
		Console.Title="C# ";
		Console.BackgroundColor= ConsoleColor.Cyan;
		Console.ForegroundColor=ConsoleColor.Black;
		int x,y,z,w,mi,b;
		float mo;
		Console.Write("Enter the first value= ");
		x=int.Parse(Console.ReadLine());
		Console.Write("Enter the second value= ");
		y=int.Parse(Console.ReadLine());
		Console.Write("Enter the third value= ");
		z=int.Parse(Console.ReadLine());
		w=x*y*z;
		Console.WriteLine("\n");
		Console.Write("The result of multiplicationis="+ " " + w);
		mo=w%2;
		Console.WriteLine("\n");
		Console.WriteLine("The Mode is="+ " " + mo);
		Console.WriteLine("\n");
		Console.Write("Enter the value of b= ");
		b=int.Parse(Console.ReadLine());
		mi=w-b;
		Console.WriteLine("The resulte of minus is=" + " " + mi);
		Console.ReadKey();
		}
	}
}



using System;
namespace g4g
{
	class ss 
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
            Console.BackgroundColor = ConsoleColor.Cyan;
            Console.ForegroundColor = ConsoleColor.Black;
            int x,y,z;
            float l,Res;
            Console.WriteLine("Enter the first value= ");
            x=Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Enter the second value= ");
            y=int.Parse(Console.ReadLine());
            z=x+y;
            Console.WriteLine("The result of Summation is="+ " " + z);
            Console.WriteLine("Enter any Float value= ");
            l=float.Parse(Console.ReadLine());
            Res=z-l;
            Console.WriteLine("The result of minus is="+ " " + Res);
            Console.ReadKey();
		}
	}
}



using System;
namespace tsif
{
	class iff
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor= ConsoleColor.Yellow;
			Console.ForegroundColor= ConsoleColor.Black;
			int x,y,z,w,Res;
			bool chek,to; 
			Console.WriteLine("Enter the first value= ");
			x=int.Parse(Console.ReadLine());
			Console.WriteLine("Enter the second value= ");
			y=int.Parse(Console.ReadLine());
			Console.WriteLine("Enter the third value= ");
			z=int.Parse(Console.ReadLine());
			Res=x+y+z;
			Console.WriteLine("The result is= " + Res);
			                                                                     /*عندما نستخدم ( || ) OR فانه يجب ان يتحقق احد شرطي ال ( if ) 
			                                                                      * عندما نستخدم ( && ) فانه يجب ان يتحقق كلا الشرطين في ال ( if )
			                                                                      */
			if(Res>=70 || Res>=100)
			{
				Console.WriteLine("The result is high");
			}
			else if(Res<50)
			{
				Console.WriteLine("The result is Low");
			}
			else
			{
				Console.WriteLine("Exit Pleas");
			}
			chek =9<3;
			to=5<9 && 3>1;
			Console.Write("\n");
			Console.WriteLine("The result of chek = "+ chek + "\n" +"The result of to= "+ to);
			Console.ReadKey();
			
		}
	}
}



using System;
namespace condo
{
	class sco
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Yellow;
			Console.ForegroundColor= ConsoleColor.Black;
			int x,y,z;
			float Res;
			Console.WriteLine("Enter the first value= ");
			x=int.Parse(Console.ReadLine());
			Console.WriteLine("Enter the second value= ");
			y=int.Parse(Console.ReadLine());
			Console.WriteLine("Enter the second value= ");
			z=int.Parse(Console.ReadLine());
			Res=((float)x-(float)y)/(float)z;
			Console.WriteLine("The result is= " + Res);
			Console.ReadKey();
		}
	}
}




using System;
namespace worl
{
	class work
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor= ConsoleColor.Yellow;
			int x,y,z;
			Console.WriteLine("Enter the first value= ");
			x=int.Parse(Console.ReadLine());
			Console.WriteLine("Enter the second value= ");
			y=int.Parse(Console.ReadLine());
			z=x+y;
			switch(z)
			{
					case 100 :Console.WriteLine("The result is high");
					break;
					case 50:Console.WriteLine("The result is medium");
					break;
					default: Console.WriteLine("The value of z= " + z +"\nwe will check it by if condition  ");
					break;
			}
			if(z>100)
						Console.WriteLine("The result is morethan the requisted");
					else
						Console.WriteLine("Please exit");
			Console.ReadKey();
		}
	}
}




using System;
namespace worl
{
	class work
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor= ConsoleColor.Yellow;
			int x,y,z;
			string t;
			Console.WriteLine("Enter the first value= ");
			x=int.Parse(Console.ReadLine());
			Console.WriteLine("Enter the second value= ");
			y=int.Parse(Console.ReadLine());
			z=x+y;
			t=z>=50? "Hi" : "The value of z=\n" + (z+100) ;
			Console.WriteLine(t);
			Console.ReadKey();
		}
	}
}




using System;
namespace worl
{
	class work
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor= ConsoleColor.Yellow;
			int i,j;
			string t;
			Console.WriteLine("Enter your Name: ");
			t=Console.ReadLine();
			Console.WriteLine("\n");
			for(i=0;i<=10;i++)
			{
				for(j=0;j<=10;j++)
				{
					Console.WriteLine(i +"*"+ j +"="+ (i*j));
				}
				Console.WriteLine("-------------------\n");
			}
			
			Console.ReadKey();
		}
	}
}



