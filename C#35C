using System;
namespace Csharp
{
    class Csharp
    {
        static void Main(string[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor=ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int i,n,f = 1;
            Console.Write("Enter n= ");
            n = int.Parse(Console.ReadLine());
            for (i = n; i >=1; i--)
            {
                f = f * i;
            }
            Console.Write("The factorial is= " + f);
            Console.ReadKey();
        }
    }
}
