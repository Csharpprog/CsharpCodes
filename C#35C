namespace Prog1
{
	class Programm
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
			int x,y,z;
			Console.WriteLine("Enter the first value= ");
			x=int.Parse(Console.ReadLine());
			Console.WriteLine("Enter the second value= ");
			y=int.Parse(Console.ReadLine());
			z=x-y;
			
			if(z>0)
				
				Console.WriteLine(z + " This Number is Positive");
			
			else
				
				Console.WriteLine(z + " This Number is Negative");
			
			Console.ReadKey();
			
		}
	}
}



using System;
namespace Prog1
{
	class Programm
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
		    char a;
                                          char c;

                                            Console.Write("Enter a Alphabet : ");
                                            a = Convert.ToChar(Console.ReadLine());

                                            c = Char.ToUpper(a);

                                            if (c.Equals('A') || c.Equals('E') || c.Equals('I') || c.Equals('O') ||
                                           c.Equals('U'))
                                           {
                                               Console.WriteLine("{0} is vowel", a);
                                           }
                                    else
                                           {
                                              Console.WriteLine("{0} is not vowel", a);
                                           }

                                         Console.ReadKey();
        }
    }
}



using System;
namespace Prog1
{
	class Programm
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
		    char x;
            Console.WriteLine("Enter any letter: ");
            x=char.Parse(Console.ReadLine());
            
            if(char.IsUpper(x))
            {
            	Console.WriteLine("This letter is Capital");
            }
            else
            {
            	Console.WriteLine("This letter is Small");
            }
            Console.ReadKey();
        }
    }
}




using System;
namespace Homepayroll
{
	class payroll
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
			int x;
			string t;
			int w=10,y=12;   //This is the number of hours that the employee work .
			Console.WriteLine("Enter the employee name: ");
			t=Console.ReadLine();
			Console.WriteLine("Enter the salary= ");
			x=int.Parse(Console.ReadLine());     //Any number thats enter by user in the execution front represent the cost of salary in Dollar( $ ) 
			if(x>=w && x<=y) 
				Console.WriteLine(t + " is the best employee");
			else
				Console.WriteLine(t + " is the careless man");
			Console.ReadKey();
		}
	}
}





using System;
namespace Homepayroll
{
	class payroll
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
			int a,b,c,Product;
			Console.WriteLine("Enter the first value= ");
			b=int.Parse(Console.ReadLine());
			Console.WriteLine("Enter the second value= ");
			c=int.Parse(Console.ReadLine());
			Product=b*c;
			Console.WriteLine("The value of Production is= " + Product);
			a=Product;
			Console.WriteLine("The value of a is= " + a);
			Console.ReadKey();
		}
	}
}




using System;
namespace star
{
	class bstr
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
			int x,y,z;
			Console.WriteLine("Enter the first value= ");
			x=int.Parse(Console.ReadLine());
			Console.WriteLine("Enter the second value= ");
			y=int.Parse(Console.ReadLine());
			Console.WriteLine("Enter the third value= ");
			z=int.Parse(Console.ReadLine());
			if(x>y)
			{
				if(x>z)
				{
				Console.WriteLine(x + " Is the biggest value");
				}
			}
			else 
				if(y>x)
			{
				if(y>z)
				{
				Console.WriteLine(y + " Is the biggest value");	
				}
				
				else
					
				Console.WriteLine(z + " Is the biggest value");
			}
			Console.ReadKey();
		}
	}
}




using System;
namespace star
{
    class bstr
    {
        static void Main(string[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            byte x, y, z;
            int Res;              //هذه يجب ان تكون من نوع integer لان نوع ال byte يحجز مكان صغير قي الذاكرة 
            Console.WriteLine("Enter the first value= ");
            x = byte.Parse(Console.ReadLine());
            Console.WriteLine("Enter the second value= ");
            y = byte.Parse(Console.ReadLine());
            Console.WriteLine("Enter the third value= ");
            z = byte.Parse(Console.ReadLine());
            Console.Write("\n");
            Res = x + y + z;
            Console.WriteLine("The result is= " + Res);
            Console.ReadKey();
        }
    }
}



