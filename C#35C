using System;
namespace CProg
{
class ProC
{
static void Main(string[] args)
{
	Console.Title="C# ";
	Console.BackgroundColor=ConsoleColor.Black;
	Console.ForegroundColor=ConsoleColor.Yellow;
       for (int i = 1; i <= 6; i++)
        {
          for (int j = 1; j <= 6-i; j++)
            Console.Write(" ");
          for (int j = 1; j <=i; j++)
            Console.Write("*");
          for (int j = i-1; j >= 1; j--)
            Console.Write("*");
            Console.Write("\n");
        }
       for (int i = 6; i >=1; i--)
       {
       	  for (int j = 1; j <= 6-i; j++)
            Console.Write(" ");
          for (int j = 1; j <=i; j++)
            Console.Write("*");
          for (int j = i-1; j >= 1; j--)
            Console.Write("*");
            Console.Write("\n");
       }
            Console.ReadKey();
     }
   }
}




