using System;
namespace tooui
{
    class hooly
    {
        static void Main(String[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int x, s,n, i, j, w;
            Console.WriteLine("Enter number of rows= ");
            x = int.Parse(Console.ReadLine());
            Console.WriteLine("Enter number of columns= ");
            n = int.Parse(Console.ReadLine());
            s = x + n - 1;
            for (i = 1; i <= x; i++)
            {
            	for (w = s; w <= i; w--)
                {
                    Console.Write(" ");
                }
                for(j=1;j<=i;j++)
                 Console.Write("* ");
                Console.Write("\n");
                s--;

            }
            Console.ReadKey();
        }
    }
}




using System;
namespace tooui
{
    class hooly
    {
        static void Main(String[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int x, s, i, j, w;
            Console.WriteLine("Enter number of rows= ");
            x = int.Parse(Console.ReadLine());
            s = x + 5 - 1;
            for (i = 1; i <= x; i++)
            {
            	for (w = s; w <= i; w--)
                {
                    Console.Write(" ");
                }
                for(j=1;j<=i;j++)
                 Console.Write("* ");
                Console.Write("\n");
                s--;

            }
            Console.ReadKey();
        }
    }
}




using System;
namespace tooui
{
    class hooly
    {
        static void Main(String[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int i,j,x;
            Console.WriteLine("Enter j col= ");
            x=int.Parse(Console.ReadLine());
            for(i=0;i<=10;i++)
            {
            	for(j=0;j<=x;j++)
            	{
            		if(j<=x-1)
            			Console.Write("{0}*{1}={2} |",j,i,i*j);
            		else
            			Console.Write("{0}*{1}={2} |",j,i,i*j);
            		
            	}
            	Console.Write("\n");
            }
            Console.ReadKey();
        }
    }
}




