using System;
using System.Runtime.CompilerServices;

namespace Csharp
{
    class Csharp
    {
        static void Main(string[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor=ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            char op;
            int x, y;
            Console.Write("Enter x= ");
            x = int.Parse(Console.ReadLine());
            Console.Write("Enter y= ");
            y = int.Parse(Console.ReadLine());
            Console.Write("Enter operation you need: \n");
            op = char.Parse(Console.ReadLine());
            if(op=='+')
                Console.Write("{0} + {1} is {2}",x,y,(x+y));
            else
            if(op=='-')
                Console.Write("x-y= " + (x-y));
            else
            if(op=='*')
                Console.Write("x*y= " + (x*y));
            else
            if(op=='/')
                Console.Write("x/y= " + (x/y)); 
            else
                Console.Write(op +" is wrong operation" );
            Console.ReadKey();
        }
    }
}

_________________________________________________________________________________________


using System;
using System.Runtime.CompilerServices;

namespace Csharp
{
    class Csharp
    {
        static void Main(string[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor=ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            char op;
            int x, y;
            Console.Write("Enter x= ");
            x = int.Parse(Console.ReadLine());
            Console.Write("Enter y= ");
            y = int.Parse(Console.ReadLine());
            Console.Write("Enter operation you need: \n");
            op = char.Parse(Console.ReadLine());
            switch (op)
            {
                case '+': Console.Write("x+y = " + (x + y));
                        break;
                case '-': Console.Write("x-y = " + (x - y));
                        break;
                case '*': Console.Write("x*y = " + (x * y));
                    break;
                case '/': Console.Write("x/y = " + (x / y));
                    break;
                default:Console.Write(op + " is wrong operation");
                    break;
            }
            Console.ReadKey();
        }
    }
}


