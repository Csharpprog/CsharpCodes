			Console.ForegroundColor=ConsoleColor.Yellow;
			int i,x,sum=0;
			Console.WriteLine("Enter the lowest value of x= ");
			x=int.Parse(Console.ReadLine());
			for(i=0;i<=x;i+=2)
			{
				sum+=i;
			}
			Console.WriteLine("The summation values under= "+ x +" is= "+sum);
