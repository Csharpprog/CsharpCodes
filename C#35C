using System;
using System.Reflection.Emit;

namespace toool
{
    class hooly
    {
        static void Main(String[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int i=0 ,sum=0;
            do
            {
                sum += i;
                ++i;
            }
            while (i>0 && i<=10);
            
            Console.WriteLine("The summation is= "+ sum);
            Console.ReadKey();
        }
    }
}




using System;
using System.Reflection.Emit;

namespace toool
{
    class hooly
    {
        static void Main(String[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int i=0,j=0;
            do
            {
            	for(;j<=10;j++)
            	{
            		Console.WriteLine(i + "*" + j + "=" + (i*j));
            	}
            	i++;
            }
            while (i>=0 && i<=10);
            Console.ReadKey();
        }
    }
}




