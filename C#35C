       for (i = 1; i <= x; i++)
        {
       	
       	l=int.Parse(Console.ReadLine());
       	sum+=l;
       }
       Console.WriteLine("The summation is= " + sum);
       avg=sum/7;
       Console.WriteLine("The average is= "+ avg);
