using System;
namespace jagged_array
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.Title="C# ";
            Console.BackgroundColor=ConsoleColor.Black;
            Console.ForegroundColor=ConsoleColor.Yellow;
            char[,] x = new char[3,3];
            int i, j;
            Console.Write("Enter array elements: \n");
            for (i = 0; i < x.GetLength(0); i++)
            {
                for (j = 0; j < x.GetLength(1); j++)
                {
                    x[i, j] = char.Parse(Console.ReadLine());
                }
            }
            for (i = 0; i < x.GetLength(0); i++)
            {
                for (j = 0; j < x.GetLength(1); j++)
                {
                    if (i-j <= j)
                    {
                     x[i, j] = '*';
                    }
                }
            }
            Console.Write("The array is: \n");
            for (i = 0; i < x.GetLength(0); i++)
            {
                for (j = 0; j < x.GetLength(1); j++)
                {
                  Console.Write(x[i,j] + " ");  
                }
                Console.Write("\n");
            }
            Console.ReadKey();
        }
    }
}




using System;
namespace sharp
{
    class PCsharp
    {
        static void Main(string[] args)
        {
            Console.Title="C# ";
            Console.BackgroundColor=ConsoleColor.Black;
            Console.ForegroundColor=ConsoleColor.Yellow;
            int[,] x = new int[3,3];
            int i, j;
            int Max = x[0,0];
            Console.Write("Enter array elements: \n");
            for (i = 0; i < x.GetLength(0); i++)
            {
                for (j = 0; j < x.GetLength(1); j++)
                {
                    x[i, j] = int.Parse(Console.ReadLine());
                }
            }
            for (i = 0; i < x.GetLength(0); i++)
            {
                for (j = 0; j < x.GetLength(1); j++)
                {
                    if (Max < x[i, j])
                    {
                        Max = x[i, j];
                    }
                }
            }
            Console.Write("\nThe array is: \n");
            for (i = 0; i < x.GetLength(0); i++)
            {
                for (j = 0; j < x.GetLength(1); j++)
                {
                    Console.Write(x[i,j] + " ");  
                }
                Console.Write("\n");
            }
            Console.Write("\nThe Maximum value is= \n" + Max);
            Console.ReadKey();
        }
    }
}




using System;
namespace exam
{
class exam
{
static void Main(string[] args)
{
Console.Title="C#";
Console.BackgroundColor=ConsoleColor.Black;
Console.ForegroundColor=ConsoleColor.Yellow;
int [] x=new int[100];
int i,j,w,n,counter=0;
Console.Write("Enter n= ");
n=int.Parse(Console.ReadLine());
Console.Write("Enter elements of array: \n");
for(i=0;i<n;i++)
{
Console.Write("Elements {0} : ",i);
x[i]=int.Parse(Console.ReadLine());
}
Console.Write("\nThe array = \n");
for(i=0;i<n;i++)
{
counter=0;
for(j=0;j<i-1;j++)
{
if(x[i]==x[j])
{
counter++;
}
}
for(w=i+1;w<n;w++)
{
if(x[i]==x[w])
{
counter++;
}
}
if(counter==0)
{
Console.Write(x[i] + " ");
}
}
Console.ReadKey();
}
}
}




using System;
namespace csharp
{
    class sharp
    {
        static void Main(string[] args)
        {
            Console.Title="C# ";
            Console.BackgroundColor=ConsoleColor.Black;
            Console.ForegroundColor=ConsoleColor.Yellow;
            int[,] x = new int[3,3];
            int i, j,sum=0;
            Console.Write("Enter array elements= \n");
            for(i=0;i<x.GetLength(0);i++)
            {
                for (j = 0; j < x.GetLength(1); j++)
                {
                    x[i, j] = int.Parse(Console.ReadLine());
                }
            }
            for (i = 0; i < x.GetLength(0); i++)
            {
                for (j = 0; j < x.GetLength(1); j++)
                {
                    if (x[i, j] % 3 == 0)
                    {
                        sum += x[i, j];
                    }
                }
            }
            Console.Write("The sum elements can divided by 3 without remainder is= " + sum);
            
        }  
    } 	
}





using System;
namespace csharp
{
    class sharp
    {
        static void Main(string[] args)
        {
            Console.Title="C# ";
            Console.BackgroundColor=ConsoleColor.Black;
            Console.ForegroundColor=ConsoleColor.Yellow;
            int[,] x = new int[3,3];
            int i, j,sum=0;
            Console.Write("Enter array elements= \n");
            for(i=0;i<x.GetLength(0);i++)
            {
                for (j = 0; j < x.GetLength(1); j++)
                {
                    x[i, j] = int.Parse(Console.ReadLine());
                }
            }
            for (i = 0; i < x.GetLength(0); i++)
            {
                for (j = 0; j < x.GetLength(1); j++)
                {
                        if (i==j || i+j==3-1)
                        {
                            sum+=x[i,j];
                        }
                    }
                }
                Console.Write("The sum of main and second diameter= " + sum);
            Console.ReadKey();
            }
        }  
    } 	




using System;

namespace sharpc
{
    class csahrp
    {
        static void Main(string[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int[] x = new int[100];
            int[] y = new int[100];
            int[] z = new int[100];
            int i, j,w,n1,n2,n3;
            Console.Write("Enter n= ");
            n1 = int.Parse(Console.ReadLine());
            for(i=0;i<n1;i++)
            {
                Console.Write("Element[{0}] : ",i);
                x[i] = int.Parse(Console.ReadLine());
            }
            Console.Write("Enter n2= ");
            n2 = int.Parse(Console.ReadLine());
            for (i = 0; i < n2; i++)
            {
                Console.Write("Element[{0}] : ",i);
                y[i] = int.Parse(Console.ReadLine());   
            }
            n3 = n1 + n2;
            for (i = 0; i < n1; i++)
            {
                z[i] = x[i];
            }
            for (j = 0; j < n2; j++)
            {
                z[i] = y[j];
                i++;
            }
            for (i = 0; i < n3; i++)
            {
                for (w = 0; w < n3 - 1; w++)
                {
                    if (z[w] >= z[w + 1])
                    {
                        j = z[w + 1];
                        z[w + 1] = z[w];
                        z[w] = j;
                    }
                }
            }
            Console.Write("The merged array: \n");
            for (i = 0; i < n3; i++)
            {
                Console.Write(z[i] + " ");
            }
            Console.ReadKey();
        }
    }
}




using System;
using System.Runtime.CompilerServices;

namespace Csharp
{
    class Csharp
    {
        static void Main(string[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor=ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int i;
            char x,y,z;
            Console.Write("Enter first letters: ");
            x = char.Parse(Console.ReadLine());
            Console.Write("Enter second letters: ");
            y = char.Parse(Console.ReadLine());
            Console.Write("Enter third letters: ");
            z = char.Parse(Console.ReadLine());
            Console.Write("The letter is {2}{1}{0}",x,y,z);
            Console.ReadKey();
        }
    }
}


