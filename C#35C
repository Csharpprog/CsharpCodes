namespace toool
{
    class hooly
    {
        static void Main(String[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int i,sum=0;
            for (i = 0; i <= 10; i++)
            {
                if(i==4)
                    break;
                sum += i;
            }
            Console.WriteLine("The summation with drop numbers after 4 is= "+ sum);
            Console.ReadKey();
        }
    }
