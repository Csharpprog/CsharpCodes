using System;
namespace array
{
	class csharry
	{
		static void Main(string[] args)
		{
			Console.Title="C# Array";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
			int i;
			int [] x=new int[5];
			x[0]=55;
			x[1]=8;
			x[2]=99;
			x[3]=12;
			x[4]=50;
			for(i=0;i<5;i++)
			{
				Console.WriteLine(x[i]);
			}
			Console.WriteLine(x[6]);   //Error
			Console.ReadKey();
		}
	}
}




