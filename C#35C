using System;
using System.Reflection.Emit;

namespace toool
{
    class hooly
    {
        static void Main(String[] args)
        {
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int x,n,sum=0,i;
            float Avg;
            Console.Write("Enter times of numbers= ");
            n=int.Parse(Console.ReadLine());
            Console.WriteLine("\nEnter numbers= ");
            for(i=1;i<=n;i++)
            {
            x=int.Parse(Console.ReadLine());
            sum+=x;
            }
            Console.Write("\n");
            Console.WriteLine("Summation of "+ n +" numbers enter by user is= "+ sum + "\n");
            Avg=sum/n;
            Console.Write("The Average of "+ n +" Numbers is= "+ Avg);
            Console.ReadKey();
        }
    }
}




