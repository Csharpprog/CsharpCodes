using System;
namespace erth
{
	class beginh
	{
		static void Main(string[] args)
		{
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            //Console.WriteLine(" Welcome to C# This is the first project ");
            int x,y,z,w;
            Console.WriteLine("Enter the first value= ");
            x=int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the second value= ");
            y=int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the third value= ");
            z=int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the fourth value= ");
            w=int.Parse(Console.ReadLine());
            if(x>=10 && y<=50)
            {
            	if(z>100 && w==90)
            		Console.WriteLine("This code is call combine if Conditions ");
            	//else
                    //Console.WriteLine("Exit");
            }
            else
                    Console.WriteLine("Exit");            
            Console.ReadKey();
	}
 }
}



using System;
namespace erth
{
	class beginh
	{
		static void Main(string[] args)
		{
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            //Console.WriteLine(" Welcome to C# This is the first project ");
            int x,y,z,w;
            Console.WriteLine("Enter the first value= ");
            x=int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the second value= ");
            y=int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the third value= ");
            z=int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the fourth value= ");
            w=int.Parse(Console.ReadLine());
            if(x>=10 && y<=50)
            {
            	if(z>100 && w==90)
            		Console.WriteLine("This code is call combine if Conditions "+  (++w) );
            	//else
                    //Console.WriteLine("Exit");
            }
            else
                    Console.WriteLine("Exit");            
            Console.ReadKey();
	}
 }
}



using System;
namespace erth
{
	class beginh
	{
		static void Main(string[] args)
		{
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            //Console.WriteLine(" Welcome to C# This is the first project ");
            int x,y,z,w;
            Console.WriteLine("Enter the first value= ");
            x=int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the second value= ");
            y=int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the third value= ");
            z=int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the fourth value= ");
            w=int.Parse(Console.ReadLine());
            if(x>=10 && y<=50)
            {
            	if(z>100 && w==90)
            		Console.WriteLine("This code is call combine if Conditions "+  (w++) );
            	//else
                    //Console.WriteLine("Exit");
            }
            else
                    Console.WriteLine("Exit");            
            Console.ReadKey();
	}
 }
}



using System;
namespace erth
{
	class beginh
	{
		static void Main(string[] args)
		{
            Console.Title = "C# ";
            Console.BackgroundColor = ConsoleColor.Black;
            Console.ForegroundColor = ConsoleColor.Yellow;
            int x,y,z,w;
            Console.WriteLine("Enter the first value= ");
            x=int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the second value= ");
            y=int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the third value= ");
            z=int.Parse(Console.ReadLine());
            Console.WriteLine("Enter the fourth value= ");
            w=int.Parse(Console.ReadLine());
            if (x >= 100 && y <=90 )
                Console.WriteLine("The value of w= " + (++w)); 
            else
                Console.WriteLine("The value of z= " + (z++));
            Console.ReadKey();
	}
 }
}



using System;
namespace hommonth
{
	class prohm
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor= ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
			int Month;
			Console.WriteLine("Enter number of Month= ");
			Month=int.Parse(Console.ReadLine());
			switch(Month)
			{
					case 1:Console.WriteLine("This month is January NO. of days is = 31 day");
					break;
				    case 2:Console.WriteLine("This month is February NO. of days is = 28 day");
				    break;
				    case 3:Console.WriteLine("This month is March NO. of days is = 31 day");
				    break;
				    case 4:Console.WriteLine("This month is April NO. of days is = 30 day");
				    break;
				    case 5:Console.WriteLine("This month is May NO. of days is = 31 day");
				    break;
				    case 6:Console.WriteLine("This month is June NO. of days is = 30 day");
				    break;
				    case 7:Console.WriteLine("This month is July NO. of days is = 31 day");
				    break;
				    case 8:Console.WriteLine("This month is August NO. of days is = 31 day");
				    break;
				    case 9:Console.WriteLine("This month is September NO. of days is = 30 day");
				    break;
				    case 10:Console.WriteLine("This month is October NO. of days is = 31 day");
				    break;
				    case 11:Console.WriteLine("This month is November NO. of days is = 30 day");
				    break;
				    case 12:Console.WriteLine("This month is December NO. of days is = 31 day");
				    break;
				    default:Console.WriteLine("You are Enter wrong choice");
				    break;
			}
			Console.ReadKey();
		}
	}
}




using System;
namespace Payrool
{
	class prog
	{
		static void Main(string[] args)
		{
			Console.Title="C# ";
			Console.BackgroundColor=ConsoleColor.Black;
			Console.ForegroundColor=ConsoleColor.Yellow;
			string t;
			Console.WriteLine("Enter your address: ");
			t=Console.ReadLine();
			if(t=="Manager")
				Console.WriteLine("Your salary is= 1000$");
			   else 
			if(t=="Assistant")
				Console.WriteLine("Your salary is= 900$");
			   else 
			if(t=="Observer")
			   	Console.WriteLine("Your salary is= 700$");
			   else
			if(t=="Employee")
			   	Console.WriteLine("Your salary is= 600$");
                                                             else			   
			   	Console.WriteLine("You don't have job");
			   Console.ReadKey();
			   			
		}
	}
}





